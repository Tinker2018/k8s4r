<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>galaxea_robot_tele</name>
  <version>0.0.0</version>
  <description>UDP-based communication between GALAXEA robot and PC</description>
  <maintainer email="your_email@example.com">Your Name</maintainer>
  <license>Apache-2.0</license>

  <buildtool_depend>ament_cmake</buildtool_depend>
  
  <build_depend>hdas_msg</build_depend>
  <exec_depend>hdas_msg</exec_depend>
  
  <depend>rclcpp</depend>
  <depend>sensor_msgs</depend>
  <depend>geometry_msgs</depend>
  <depend>std_srvs</depend> <!-- 已添加：标准服务依赖 -->
  <depend>system_manager_msg</depend> <!-- 已添加：系统管理服务依赖 -->
  <depend>ament_index_cpp</depend>
  <depend>flatbuffers</depend>
  <depend>yaml-cpp</depend>

  <test_depend>ament_lint_auto</test_depend>
  <test_depend>ament_lint_common</test_depend>

  <export>
    <build_type>ament_cmake</build_type>
  </export>
</package>